package Ph2_CSC212;

public class AVLNodeMap<K extends Comparable<K>, T> {
	

	    public K key;
	    public T data;
	    public Balance bal;
	    public AVLNodeMap<K,T> left;
	    public AVLNodeMap<K,T> right;

	    public AVLNodeMap(K key, T data) {
	        this.key = key;
	        this.data = data;
	        this.bal = Balance.Zero;
	        this.left = null;
	        this.right = null;
	    }
	}



