private void printAllKeysDataRec(AVLNode<T> node) {
if(node == null)
	return;

printAllKeysDataRec(node.left);


if(node.data instanceof Customer) {
	Customer c = (Customer) node.data;
	System.out.println("Customer ID : " + c.getCustomerId() + " Name : "+ c.getName() + " Email : " + c.getEmail());
}
else if (node.data instanceof Order) {
	Order o = (Order) node.data;
	System.out.println();	
}

else if (node.data instanceof Product) {
	Product p = (Product) node.data;
	System.out.println("Product Id : "+p.getProductId() + " Name : "+ p.getName() + " Price : "+ p.getPrice());	
}

printAllKeysDataRec(node.right);
 }
