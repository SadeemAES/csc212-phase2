package csc212_phase2;

public class AVLNode<T> {
	   public int key;             
	   public T data;               
	   public Balance bal;              
	   public AVLNode<T> left;
	   public AVLNode<T> right;

	    public AVLNode(int key, T data) {
	        this.key = key;
	        this.data = data;
	        this.bal = Balance.Zero;
	        this.left = null;
	        this.right = null;
	    } 
	    
	}   
